
@{
    ViewData["Title"] = "Recipes";
}

@model VModel
@using Eatery.DAL


<partial name="PartialViewStaticSlider" model="@Model.StaticSlider.FirstOrDefault(x => x.ActionName == "Recipes")" />

<partial name="PartialViewOurMenu" model="Model.OurMenus"/>

<section class="section bg-light element-animate">
    <div class="container">
        <div class="row">
            @foreach (var Et in Model.EatingTypes.Take(4))
            {
                <partial name="/Views/Shared/EatingTypePartialView.cshtml" for="@Et" />
            }


        </div>
    </div>
</section>

<partial name="/Views/Shared/SubscribePartialView.cshtml" />