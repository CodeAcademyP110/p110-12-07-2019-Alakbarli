
@{
    ViewData["Title"] = "Dcreate";
}



@using Eatery.Models
@model Slider
@{
    ViewData["Title"] = "Screate";
}
<h2>Create a new  Slider</h2>

<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Image">Image</label>
        <input asp-for="Image" class="form-control">
        <span asp-validation-for="Image"></span>
    </div>

    <div class="form-group">
        <label asp-for="Heading">Heading</label>
        <input asp-for="Heading" type="text" class="form-control">
        <span asp-validation-for="Heading"></span>

    </div>
    <div class="form-group">
        <label asp-for="Content">Heading</label>
        <input asp-for="Content" type="text" class="form-control">
        <span asp-validation-for="Content"></span>

    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>
